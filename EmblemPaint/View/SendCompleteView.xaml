<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel="clr-namespace:EmblemPaint.ViewModel">
    <DataTemplate DataType="{x:Type viewModel:SendCompleteViewModel}">
        <Grid>
            <Grid.Background>
                <ImageBrush ImageSource="../Images/SendComplete.jpg"/>
            </Grid.Background>
            <Grid.RowDefinitions>
                <RowDefinition Height="9.4*"/>
                <RowDefinition/>
                <RowDefinition Height="6*"/>
            </Grid.RowDefinitions>
            <Button Grid.Row="1" Width="934" Style="{StaticResource SoundActionButton}" Command="{Binding HomeCommand}">
                <Image Source="../Images/v_nachalo.png"></Image>
            </Button>
        </Grid>
    </DataTemplate>
    
</ResourceDictionary>